{
	"name": "reatom-dnd",
	"version": "0.0.1",
	"description": "Drag and drop library for Reatom",
	"type": "module",
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"import": "./dist/index.mjs"
		},
		"./react": {
			"types": "./dist/react.d.mts",
			"import": "./dist/react.mjs"
		}
	},
	"files": ["dist"],
	"sideEffects": false,
	"scripts": {
		"start": "pnpm --filter reatom-dnd-examples dev",
		"build": "tsdown",
		"prepublishOnly": "pnpm build",
		"examples": "pnpm --filter reatom-dnd-examples dev",
		"docs": "typedoc",
		"docs:serve": "npx http-server docs -o"
	},
	"peerDependencies": {
		"@reatom/core": ">=1000.0.0",
		"react": ">=18.0.0"
	},
	"peerDependenciesMeta": {
		"react": {
			"optional": true
		}
	},
	"devDependencies": {
		"@reatom/core": "^1000.1.0",
		"@types/react": "^19.0.0",
		"react": "^19.0.0",
		"tsdown": "0.20.0-beta.3",
		"typedoc": "^0.27.5",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"mitt": "^3.0.1"
	},
	"keywords": ["reatom", "dnd", "drag-and-drop"],
	"license": "MIT",
	"author": "khmilevoi",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/khmilevoi/reatom-dnd.git"
	},
	"homepage": "https://github.com/khmilevoi/reatom-dnd#readme",
	"bugs": {
		"url": "https://github.com/khmilevoi/reatom-dnd/issues"
	}
}
